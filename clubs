                                                                                                                                                                                    `]; // <- Insert your real frames here if different

    function fitTextToContainer(text, fontFace, containerWidth) {
      const PIXEL_RATIO = window.devicePixelRatio || 1;
      let canvas = document.createElement("canvas");
      let context = canvas.getContext("2d");

      canvas.width = containerWidth * PIXEL_RATIO;
      canvas.height = 1;
      context.setTransform(PIXEL_RATIO, 0, 0, PIXEL_RATIO, 0, 0);

      const getLongestLine = (lines) => lines.reduce((a, b) => a.length > b.length ? a : b);
      const longestLine = getLongestLine(text.split('\n'));
      let fontSize = 300;

      do {
        context.font = `${fontSize}px ${fontFace}`;
        fontSize--;
      } while (context.measureText(longestLine).width > canvas.width && fontSize > 5);

      return fontSize / (PIXEL_RATIO / 1.62);
    }

    const pre = document.getElementById('trace');
    let i = -1;
    let dir = 'inc';
    const max = frames.length;
    const fps = 4;

    function setPreCharSize() {
      const charRatio = 0.66;
      const charWidth = fitTextToContainer(frames[0].split('\n')[1], 'monospace', pre.clientWidth) * charRatio;
      const charHeight = charRatio * charWidth;
      pre.style.fontSize = charWidth + "px";
      pre.style.lineHeight = charHeight + "px";
    }

    function step() {
      i = (dir === 'inc') ? i + 1 : i - 1;
      if (i >= max) { dir = 'dec'; i = max - 2; }
      if (i < 0) { dir = 'inc'; i = 1; }
      document.getElementById('trace-chars').innerText = frames[i];
    }

    function animate() {
      const fpsInterval = 1000 / fps;
      let then = Date.now();

      function loop() {
        requestAnimationFrame(loop);
        const now = Date.now();
        const elapsed = now - then;

        if (elapsed > fpsInterval) {
          then = now - (elapsed % fpsInterval);
          step();
        }
      }

      loop();
    }

    setPreCharSize();
    animate();
    window.addEventListener('resize', setPreCharSize);
  </script>
</div>
